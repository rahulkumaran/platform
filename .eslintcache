led(NavIcon)`\nmargin-right: 10px;\n@media (min-width: 850px) {\n display: none;\n}\n`\n\nconst SocialAnchor = styled.a`\n  margin-right: 20px;\n  &:hover {\n    opacity: 0.5;\n  }\n`\nconst SocialsWrapper = styled.div`\n  display: flex;\n  justify-content: space-evenly;\n\n  @media (max-width: 850px) {\n    display: none;\n  }\n`\nconst Logo = styled.h2`\n  color: white;\n`","/Users/AdeeshSharma/Desktop/web3/NFT-LP/src/components/nav/SidebarData.js",[],"/Users/AdeeshSharma/Desktop/web3/NFT-LP/src/components/nav/SubMenu.js",[],"/Users/AdeeshSharma/Desktop/web3/NFT-LP/src/redux/store.js",[],"/Users/AdeeshSharma/Desktop/web3/NFT-LP/src/redux/blockchain/blockchainReducer.js",[],"/Users/AdeeshSharma/Desktop/web3/NFT-LP/src/redux/data/dataReducer.js",[],{"ruleId":"62","severity":1,"message":"63","line":3,"column":8,"nodeType":"64","messageId":"65","endLine":3,"endColumn":14},{"ruleId":"66","severity":1,"message":"67","line":32,"column":17,"nodeType":"68","messageId":"69","endLine":32,"endColumn":19},{"ruleId":"62","severity":1,"message":"70","line":1,"column":17,"nodeType":"64","messageId":"65","endLine":1,"endColumn":25},{"ruleId":"62","severity":1,"message":"71","line":32,"column":7,"nodeType":"64","messageId":"65","endLine":32,"endColumn":10},{"ruleId":"62","severity":1,"message":"72","line":42,"column":7,"nodeType":"64","messageId":"65","endLine":42,"endColumn":14},{"ruleId":"62","severity":1,"message":"73","line":74,"column":7,"nodeType":"64","messageId":"65","endLine":74,"endColumn":19},{"ruleId":"62","severity":1,"message":"74","line":80,"column":7,"nodeType":"64","messageId":"65","endLine":80,"endColumn":21},{"ruleId":"62","severity":1,"message":"75","line":88,"column":7,"nodeType":"64","messageId":"65","endLine":88,"endColumn":11},{"ruleId":"62","severity":1,"message":"70","line":1,"column":17,"nodeType":"64","messageId":"65","endLine":1,"endColumn":25},"no-unused-vars","'styled' is defined but never used.","Identifier","unusedVar","eqeqeq","Expected '===' and i