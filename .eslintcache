s.js",["87"],"import Web3 from 'web3'\nimport SmartContract from '../../contracts/test_contract.json'\n\nimport { fetchData } from '../data/dataActions'\n\nconst connectRequest = () => {\n return {\n  type: 'CONNECTION_REQUEST'\n }\n}\n\nconst connectSuccess = payload => {\n return {\n  type: 'CONNECTION_SUCCESS',\n  payload: payload\n }\n}\n\nconst connectFailed = payload => {\n return {\n  type: 'CONNECTION_FAILED',\n  payload: payload\n }\n}\n\nconst updateAccountRequest = payload => {\n return {\n  type: 'UPDATE_ACCOUNT',\n  payload: payload\n }\n}\n\nexport const disconnect = () => {\n return {\n  type: 'DISCONNECT'\n }\n}\n\nexport const connect = () => {\n return async dispatch => {\n  dispatch(connectRequest())\n  const { ethereum } = window\n  const metamaskIsInstalled = ethereum && ethereum.isMetaMask\n\n  if (metamaskIsInstalled) {\n   let web3 = new Web3(ethereum)\n   try {\n    const accounts = await ethereum.request({\n     method: 'eth_requestAccounts'\n    })\n    const networkId = await ethereum.request({\n     method: 'net_version'\n    })\n\n    if (networkId == 250) {\n     const SmartContractObj = new web3.eth.Contract(\n      SmartContract,\n      'CONTRACT_ADDR_GOES HERE'\n     )\n     dispatch(\n      connectSuccess({\n       account: accounts[0],\n       smartContract: SmartContractObj,\n       web3: web3\n      })\n     )\n\n     // Add listeners start\n     ethereum.on('accountsChanged', accounts => {\n      dispatch(updateAccount(accounts[0]))\n     })\n     ethereum.on('chainChanged', () => {\n      window.location.reload()\n     })\n     // Add listeners end\n    } else {\n     alert('Please Connect To Fantom Mainnet Network! [Chain ID:250]')\n     dispatch(\n      connectFailed(\n       'Please Connect To Fantom Mainnet Network! [Chain ID:250]'\n      )\n     )\n    }\n   } catch (err) {\n    alert('An unexpected Error Occurred.')\n    dispatch(connectFailed('Something went wrong.'))\n   }\n  } else {\n   alert('Please Install Metamask on your device.')\n   dispatch(connectFailed('Install Metamask.'))\n  }\n }\n}\n\nexport const updateAccount = account => {\n return async dispatch => {\n  dispatch(updateAccountRequest({ account: account }))\n  dispatch(fetchData(account))\n }\n}\n","/Users/AdeeshSharma/Desktop/web3/NFT-LP/src/redux/data/dataActions.js",[],"/Users/AdeeshSharma/Desktop/web3/NFT-LP/src/pages/collection-two.js",[],"/Users/AdeeshSharma/Desktop/web3/NFT-LP/src/components/commons/Jumbotron.js",["88"],"import React from 'react'\nimport styled from 'styled-components'\nimport { Container } from 'react-bootstrap'\n\n\nconst Div = styled.div``\n\nexport const Jumbotron = ({ title, text, image }) => (\n  <Div>\n    <Jumbo bg={image}>\n      <InnerDiv class=\"p-5\">\n        <h1 class=\"display-4 fw-bold\" style={{ animation: 'float infinite 1.5s' }}>{title}</h1>\n        <hr />\n        <p>{text}</p>\n      </InnerDiv>\n    </Jumbo>\n  </Div>\n)\n\nconst Jumbo = styled.div`\n${props =>\n    props.bg ? `background-image: url(${props.bg})` : ''}; background-color: rgba(86, 194, 50, .3)!important;\n  background-size: cover;\n  background-repeat: no-repeat;\n  margin-bottom: 30px !important;\n  border-radius: 20px;\n\n  margin-top: 50px !important;\n  margin-bottom: 50px !important;\n  display: block;\n  padding: 30px;\n  box-shadow: rgba(0, 0, 0, 0.35) 0px 5px 15px;`\n\nconst InnerDiv = styled.div`\npadding: 50px;\n`","/Users/AdeeshSharma/Desktop/web3/NFT-LP/src/components/cards/index.js",["89"],"import React from 'react'\nimport { Container, Card, Row, Col } from 'react-bootstrap'\nimport styled from 'styled-components'\n\nconst Cards = ({ team }) => {\n return (\n\n  <Row xs={1} md={2} l={2} xl={3} className=\"g-5\">\n   {team.map((_, idx) => (\n    <Col>\n     <CustomCard border=\"secondary\">\n      <CardImg variant=\"top\" src=\"https://cdn.pixabay.com/photo/2015/04/23/22/00/tree-736885__480.jpg\" />\n      <Card.Body>\n       <Card.Title>{_.name}</Card.Title>\n       <Card.Text>\n        